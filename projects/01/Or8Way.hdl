/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=in[0],b=in[1],out=Or1);
    Or(a=in[2],b=Or1,out=Or2);
    Or(a=in[3],b=Or2,out=Or3);
    Or(a=in[4],b=Or3,out=Or4);
    Or(a=in[5],b=Or4,out=Or5);
    Or(a=in[6],b=Or5,out=Or6);
    Or(a=in[7],b=Or6,out=out);
}