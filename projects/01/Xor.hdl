/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Or(a=a,b=b,out=outOr);
	Nand(a=a,b=b,out=outAnd);
	And(a=outOr,b=outAnd,out=out);
}